 = np.array(posVal)                                # Преобразуем в numpy\n","    indexes = posVal > 0.2                                   # Берем индексы тех позиций, доля которых составляет больше 20%\n","    indexedPos = pos[indexes]                                # Выбираем эти позиции из pos\n","    indexedPosVal = posVal[indexes]                          # Выбираем доли этих позиций\n","\n","    # Отображаем долю позиции и название самой позиции\n","    c = 0\n","    for i in range(len(indexedPos)):\n","      c += 1\n","      print(round(100*indexedPosVal[i]), \"% \", indexedPos[i], sep=\"\")\n","    \n","    print(\"Количество позиций, которые есть в 20% корзин:\", c)\n","\n","    # Ищем слова, которые встречаются более чем в 20% корзин\n","    \n","    # Разбиваем описание позиции на слова\n","    a_pos = []\n","    for p in pos:\n","      p = p[0]\n","      p = p.replace(\")\", \" \")\n","      p = p.replace(\"(\", \" \")\n","      p = p.replace(\"-\", \" \")\n","      p = p.replace(\"+\", \" \")\n","      p = p.replace(\"/\", \" \")\n","      p = p.replace(\"\\\\\", \" \")\n","      p = p.replace(\",\", \" \")\n","      p = p.replace(\".\", \" \")\n","      p = p.replace(\"*\", \" \")\n","      p = p.replace(\"\\\"\", \" \")\n","      p = p.replace(\"'\", \" \")\n","      p = p.replace(\"=\", \" \")\n","      p = p.replace(\":\", \" \")\n","      p = p.replace(\";\", \" \")\n","      p = p.replace(\"  \", \" \")\n","      p = p.replace(\"  \", \" \")\n","      p = p.replace(\"  \", \" \")\n","\n","      # Разбиваем на слова и записываем в архив позиций\n","      a_pos.append(p.split())\n","    \n","    # составляем словарь и присваиваем нулевые значения\n","    stopw = ['а', 'в', 'на', 'от', 'с', 'уп', 'пар', 'мл', 'для'] # словарик стоп-слов\n","\n","    words = {}\n","    for p in a_pos:\n","      for word in p:\n","        if not word in stopw:\n","          words[word] = 0\n","\n","\n","\n","    for word in words:\n","      for pwords in a_pos:\n","        for pword in pwords:\n","          if (word == pword):\n","            words[word] = words[word] + 1\n","            break\n","\n","    # Сортируем словарик по количеству вхождений слов в позиции\n","    swords = list(words.items())\n","    swords.sort(key=lambda i: i[1], reverse=True)\n","\n","    print(\"Слова, вскречающиеся более чем в 20% товарных позиций:\")\n","    ss = 0\n","    for word in swords:\n","      word = word[0]\n","      per = words[word] * 100 / len(pos)\n","      if (per >= 20):\n","        print(str(round(per, 2)) + \"% \" + word)\n","      else:\n","        break\n","\n","\n","    # Берем все позиции, которые в единственном числе\n","    wc = 0\n","    for word in swords:\n","      word = word[0]\n","      if (words[word] == 1):\n","        wc += 1\n","\n","    uniq.append(wc)\n","\n","    print()\n","    print()"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Покупок (корзин): 44\n","Позиций всего: 154\n","100% ['Окислительная эмульсия  1,9% Londa 60 мл.']\n","Количество позиций, которые есть в 20% корзин: 1\n","Слова, вскречающиеся более чем в 20% товарных позиций:\n","\n","\n","Покупок (корзин): 152\n","Позиций всего: 473\n","100% ['Шампунь для ежедневного применения 1000 мл. Curex Classic']\n","Количество позиций, которые есть в 20% корзин: 1\n","Слова, вскречающиеся более чем в 20% товарных позиций:\n","38.05% волос\n","\n","\n","Покупок (корзин): 15\n","Позиций всего: 191\n","53% ['2 фазный спрей \"Cердце океана\" термозащита+увлажнение 200 мл.']\n","27% ['Крем-бальзам-архитектор волос  для восстановления + питания (кератиновый ремонт волос) 1000 мл.']\n","27% ['Крем-бальзам-архитектор волос  для восстановления + питания (кератиновый ремонт волос) 200 мл.']\n","33% ['Маска-глина архитектор волос реконструкция + регенерация 500 мл.']\n","33% ['Нектар спрей Top-finish разглаживание, уплотнение, ламинирование 200 мл.']\n","47% ['Органик бальзам-маска  гиалуроновая 200 мл']\n","33% ['Органик бальзам-м